<Window x:Class="fnbot.shop.Changelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fnbot.shop"
        mc:Ignorable="d"
        Title="Changelog" Height="400" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBlock MouseLeftButtonUp="Click_Name" HorizontalAlignment="Right" Margin="0,10,0,0" Text="fnbot.shop" VerticalAlignment="Top" Height="33" Width="300" TextAlignment="Center" FontSize="24" FontFamily="Segoe UI"/>
        <TextBlock x:Name="TextData" Margin="10,40,10,0" Text="fnbot.shop" TextWrapping="Wrap" FontSize="16" FontFamily="Segoe UI"/>
    </Grid>
</Window>
